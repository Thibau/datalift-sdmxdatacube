style(type='text/css')
  *{margin:0;padding:0}body{font-family:arial;font-size:12px;background:#ccc;color:#333}header{padding:0 0 10px 0;border-bottom:1px solid #bbb;margin:0 0 10px 0}.container{font-size:24px;font-weight:bold;font-style:italic;margin:0 0 7px 0;display:block;clear:left}article{margin:30px 50px;background:#fff;padding:10px 20px 20px 20px;border-radius:5px;border:2px solid #bbb;overflow:hidden}section{clear:left;margin:0 20px 7px 20px}headergroup{clear:left;display:block;margin:0 0 0 17px}headergroup p{width:600px}h1{font-size:16px;margin:0 0 4px 0}h2{font-size:14px;font-style:italic;color:#09c;margin:0 0 3px 0;display:inline-block;cursor:pointer}h3{font-size:12px;margin:3px 0}p{margin:0 0 7px 0}dl{margin:0 0 7px 17px;clear:left}dt{padding:1px 0;line-height:18px;position:relative;float:left;clear:left;display:inline-block;font-weight:bold;width:150px}dd{padding:1px 0;line-height:18px}dd.new-block{display:block;clear:left}dt.property{cursor:pointer;color:#09c}span.required{display:inline-block;margin:0 0 0 -17px;color:red;font-weight:bold;float:left;font-size:10px}footer{padding:10px 0 0 0;border-top:1px solid #333}
